#include <stdio.h>

int main() {
    float n1, n2, n3, media;

    printf("Digite a n1: ");
    scanf("%f", &n1);
    printf("Digite a n2: ");
    scanf("%f", &n2);
    printf("Digite a n3: ");
    scanf("%f", &n3);

    media = (n1 + n2 + n3) / 3;
    printf("media = %.2f\n", media);

    if (media >= 6) {
        printf("Aprovado!\n");
    } else {
        printf("Reprovado!\n");
    }

    return 0;
}
